mages/logo-dark.svg');\r\n        //     }\r\n        // };\r\n\r\n        // onMounted(() => {\r\n        //     updateLogo();\r\n        // });\r\n\r\n        // watch(() => isDarkTheme, () => {\r\n        //     updateLogo();\r\n        // });\r\n\r\n        return { logo };\r\n    },\r\n    components: {\r\n        simplebar\r\n    },\r\n    data() {\r\n        return {\r\n            towerId : 0,\r\n        };\r\n    },\r\n    methods: {\r\n        changeLayoutType(layoutType) {\r\n            // Update the layout type in the store\r\n            this.$store.commit('changeLayoutType', { layoutType });\r\n\r\n            // Set the layout attribute based on the layout type\r\n            document.body.setAttribute('data-pc-layout', layoutType);\r\n        },\r\n    },\r\n    computed: {\r\n        // ...layoutComputed,\r\n        layoutType: {\r\n            get() {\r\n                return this.$store.state.layout.layoutType;\r\n            },\r\n            set(layoutType) {\r\n                this.$store.commit('changeLayoutType', { layoutType });\r\n            },\r\n        },\r\n    },\r\n    watch: {\r\n        layoutType: {\r\n            immediate: true,\r\n            deep: true,\r\n            handler(newVal, oldVal) {\r\n                if (newVal !== oldVal) {\r\n                    switch (newVal) {\r\n                        case \"horizontal\":\r\n                            document.body.setAttribute(\r\n                                \"data-pc-layout\",\r\n                                \"horizontal\"\r\n                            );\r\n                            break;\r\n                        case \"vertical\":\r\n                            document.body.setAttribute(\"data-pc-layout\", \"vertical\");\r\n                    }\r\n                }\r\n            },\r\n        },\r\n    },\r\n    mounted() {\r\n        const idtower = this.$route.params.idtower;\r\n          //Giải mã\r\n          //const DecodingIdTower = CryptoJS.enc.