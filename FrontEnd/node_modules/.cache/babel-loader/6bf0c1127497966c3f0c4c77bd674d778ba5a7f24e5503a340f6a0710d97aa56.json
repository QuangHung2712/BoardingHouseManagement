{"ast":null,"code":"import pageheader from \"@/components/PageHeader.vue\";\nimport apiClient from \"@/plugins/axios\";\nimport VueEasyLightbox from \"vue-easy-lightbox\";\nimport { formatTablePrice } from \"@/components/common/JavaScripCommon\";\nexport default {\n  name: \"LANDING\",\n  components: {\n    pageheader,\n    VueEasyLightbox\n  },\n  data() {\n    return {\n      length: 3,\n      window: 0,\n      currentSlide: 0,\n      visible: false,\n      index: 0,\n      roomData: {},\n      message: '',\n      snackbar: false,\n      snackbarColor: ''\n    };\n  },\n  created() {\n    const roomId = this.$route.params.idroom;\n    this.GetDetailRoom(roomId);\n  },\n  methods: {\n    showImg(index) {\n      this.index = index;\n      this.visible = true;\n    },\n    handleHide() {\n      this.visible = false;\n    },\n    GetDetailRoom(roomId) {\n      apiClient.get(`/Room/GetDetailFindRoom?roomId=${roomId}`).then(response => {\n        this.roomData = response.data;\n        console.log(this.roomData);\n      }).catch(error => {\n        this.message = \"Lấy thông tin phòng bị lỗi: \" + error.response?.data?.message || error;\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n      });\n    },\n    FormatPrice(price) {\n      return formatTablePrice(price);\n    },\n    GoToInfoZalo() {\n      const url = 'https://zalo.me/' + this.roomData.sdtZalo;\n      window.open(url, '_blank');\n    }\n  }\n};","map":{"version":3,"names":["pageheader","apiClient","VueEasyLightbox","formatTablePrice","name","components","data","length","window","currentSlide","visible","index","roomData","message","snackbar","snackbarColor","created","roomId","$route","params","idroom","GetDetailRoom","methods","showImg","handleHide","get","then","response","console","log","catch","error","FormatPrice","price","GoToInfoZalo","url","sdtZalo","open"],"sources":["D:\\Du_An\\BoardingHouseManagement\\FrontEnd\\src\\views\\FindRoom\\DetailRoom.vue"],"sourcesContent":["<style scoped>\r\n    .thumbnail-container {\r\n        cursor: pointer;\r\n        padding: 4px;\r\n    }\r\n\r\n    .thumbnail {\r\n        border: 2px solid transparent;\r\n        border-radius: 8px;\r\n        transition: 0.3s;\r\n    }\r\n\r\n    .thumbnail.active {\r\n        border-color: #1976d2; /* Màu xanh Vuetify */\r\n        box-shadow: 0 0 8px rgba(25, 118, 210, 0.7);\r\n    }\r\n\r\n    .thumbnail {\r\n        width: 50px;\r\n        height: 50px; \r\n    }\r\n    .item{\r\n        background-color: #f1f4f6;\r\n    }\r\n    .d-grid button{\r\n        font-size: 18px;\r\n    }\r\n    .customBtn{\r\n        background-color: green;\r\n        border-color: green;\r\n    }\r\n</style>\r\n<script>\r\n    import pageheader from \"@/components/PageHeader.vue\";\r\n    import apiClient from \"@/plugins/axios\";\r\n    import VueEasyLightbox from \"vue-easy-lightbox\";\r\n    import { formatTablePrice } from \"@/components/common/JavaScripCommon\"\r\n\r\n\r\n    export default {\r\n        name: \"LANDING\",\r\n        components: {\r\n            pageheader,VueEasyLightbox\r\n        },\r\n    data() {\r\n        return{\r\n            length: 3,\r\n            window: 0,\r\n            currentSlide: 0,\r\n            visible: false,\r\n            index: 0,\r\n            roomData:{},\r\n            message: '',\r\n            snackbar: false,\r\n            snackbarColor: '',\r\n        }\r\n    },\r\n    created(){\r\n        const roomId = this.$route.params.idroom;\r\n        this.GetDetailRoom(roomId);\r\n    },\r\n    methods:{\r\n        showImg(index) {\r\n            this.index = index;\r\n            this.visible = true;\r\n        },\r\n        handleHide() {\r\n            this.visible = false;\r\n        },\r\n        GetDetailRoom(roomId){\r\n            apiClient.get(`/Room/GetDetailFindRoom?roomId=${roomId}`)\r\n                .then(response=>{\r\n                    this.roomData = response.data;\r\n                    console.log(this.roomData);\r\n                })\r\n                .catch(error=>{\r\n                    this.message = \"Lấy thông tin phòng bị lỗi: \" + error.response?.data?.message || error;\r\n                    this.snackbar = true;\r\n                    this.snackbarColor = 'red';\r\n                })\r\n        },\r\n        FormatPrice(price){\r\n            return formatTablePrice (price);\r\n        },\r\n        GoToInfoZalo(){\r\n            const url = 'https://zalo.me/' + this.roomData.sdtZalo;\r\n            window.open(url,'_blank');\r\n        }\r\n    }\r\n}\r\n</script>\r\n<template>\r\n        <v-snackbar\r\n            v-model=\"snackbar\"\r\n            :timeout=\"10000\"\r\n            class=\"custom-snackbar\"\r\n            :color=\"snackbarColor\"\r\n        >\r\n            <h5 class=\"text-center\">{{ message }}</h5>\r\n        </v-snackbar>\r\n        <v-row class=\"m-4\"> \r\n            <v-col cols=\"12\" xxl=\"11\">\r\n                <pageheader title=\"\" pageTitle=\"Chi tiết phòng trọ\" />\r\n                <BRow>\r\n                    <BCol class=\"col-lg-8\">\r\n                        <BCard no-body class=\"mt-2\">\r\n                            <v-row justify=\"center\" class=\"m-0\" >\r\n                                <!-- Carousel chính -->\r\n                                <v-carousel v-model=\"currentSlide\" hide-delimiters show-arrows=\"hover\" height=\"480\" class=\"item\" >\r\n                                    <v-carousel-item v-for=\"(item, index) in roomData.pathImgRoom\" :key=\"index\">\r\n                                        <v-img :src=\"item\" @click=\"showImg(index)\" />\r\n                                    </v-carousel-item>\r\n                                </v-carousel>\r\n\r\n                                <!-- Thumbnails -->\r\n                                <v-row justify=\"center\" class=\"m-0\">\r\n                                    <div\r\n                                    v-for=\"(item, index) in roomData.pathImgRoom\"\r\n                                    :key=\"index\"\r\n                                    @click=\"currentSlide = index\"\r\n                                    class=\"thumbnail-container\"\r\n                                    >\r\n                                    <v-img\r\n                                        :src=\"item\"\r\n                                        :alt=\"'Ảnh phòng ' + index \"\r\n                                        class=\"thumbnail\"\r\n                                        :class=\"{ active: currentSlide === index }\"\r\n                                    />\r\n                                    </div>\r\n                                </v-row>\r\n                            </v-row>\r\n                        </BCard>\r\n                        <BCard>\r\n                            <BCardHeader >\r\n                                <h4>{{ roomData.towerName }}</h4>\r\n                                <p><v-icon>mdi-map-marker-radius</v-icon>{{ roomData.towerAddress }}</p>\r\n                                <h3>Giá: <span style=\"color: red;\">{{ roomData.priceRoom }}</span></h3>\r\n                            </BCardHeader>\r\n                            <BCardBody class=\"p-0 py-3\">\r\n                                <h4>Thông tin thiết bị</h4>\r\n                                <p>Các thiết bị của phòng: {{ roomData.device }} </p>\r\n                                <h4>Thông tin dịch vụ</h4>\r\n                                <BRow>\r\n                                    <BCol class=\"col-lg-4\" v-for=\"(service,index) in roomData.service\" :key=\"index\">\r\n                                        <p>+ {{ service.name }}: {{ FormatPrice(service.price) }}/{{ service.unitOfCalculation }}</p>\r\n                                    </BCol>\r\n                                </BRow>\r\n                                <h4>Mô tả</h4>\r\n                                <p>{{ moTa }}</p>\r\n                            </BCardBody>\r\n                        </BCard>\r\n                    </BCol>\r\n                    <BCol class=\"col-lg-4\">\r\n                        <BCard class=\"statistics-card-1\" no-body>\r\n                            <BCardBody class=\"text-center\">\r\n                                <div class=\"chat-avtar d-inline-flex mx-auto\">\r\n                                    <img class=\"rounded-circle img-fluid wid-90 img-thumbnail\"\r\n                                        :src=\"roomData.landlordAvata\" alt=\"User image\">\r\n                                </div>\r\n                                <h5 class=\"mb-0\">{{ roomData.landlordName }}</h5>\r\n                                <div class=\"d-grid gap-2 mt-3 \">\r\n                                    <BButton class=\"customBtn\"><v-icon class=\"mr-2\">mdi-phone</v-icon>{{ roomData.sdtLandlord }}</BButton>\r\n                                </div>\r\n                                <div class=\"d-grid gap-2 mt-3\">\r\n                                    <BButton variant=\"primary\" @click=\"GoToInfoZalo\"><v-icon class=\"mr-2\">mdi-chat-processing-outline</v-icon>Nhắn tin Zalo</BButton>\r\n                                </div>\r\n                                <div class=\"d-flex justify-space-evenly mt-3\">\r\n                                    <BButton variant=\"white\" class=\"p-0\"><v-icon class=\"mr-2\">mdi-heart-outline</v-icon>Lưu phòng</BButton>\r\n                                    <BButton variant=\"white\" class=\"p-0\"><v-icon class=\"mr-2\">mdi-share-variant</v-icon>Chia sẻ phòng</BButton>\r\n                                </div>\r\n                            </BCardBody>\r\n                        </BCard>\r\n                    </BCol>\r\n                </BRow>\r\n            </v-col>\r\n        </v-row>\r\n    <vue-easy-lightbox :visible=\"visible\" :imgs=\"roomData.pathImgRoom\" :index=\"index\" @hide=\"handleHide\"></vue-easy-lightbox>\r\n</template>"],"mappings":"AAiC