      btnEdit(){\r\n                this.dialogEdit=true,\r\n                this.titleDialog='Sửa nhà trọ'\r\n            },\r\n            goToTowerDetails(idtower) {\r\n                //Mã hóa Id của tower\r\n                const encryptedId = CryptoJS.AES.encrypt(idtower, 'your-secret-key').toString();\r\n                this.$router.push({ name: 'towerDetails', params: { idtower: encryptedId } });\r\n            },\r\n            GetAllTower(){\r\n                apiClient.get(`/Tower/GetAllTowerByLandlordId/${1}`)\r\n                    .then(reponse =>{\r\n                        this.towerData =reponse.data\r\n                        console.log(this.towerData)\r\n                    })\r\n                    .catch(error => {\r\n                        console.error('Lấy thông tin của tòa nhà bị lỗi: ',error);\r\n                    })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n.m0{\r\n    margin: 0px;\r\n}\r\n.p0{\r\n    padding: 0px;\r\n}\r\n.itemTower{\r\n    margin: 1%;\r\n    border-radius: 25px;\r\n}\r\n.dialog{\r\n    width: 80%;\r\n    height: 80vh;\r\n}\r\n.dialog .v-btn{\r\n    background-color: blue;\r\n    color: whitesmoke;\r\n}\r\n</style>"],"mappings":";AAqCI,OAAOA,QAAO,MAAO,WAAW;AAChC,OAAOC,SAAQ,MAAO,uBAAuB;AAC7C,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,SAAS,EAAE,EAEV;MACDC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,WAAW,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAC;IACJC,MAAMA,CAAA,EAAE;MACJ,IAAI,CAACL,UAAU,GAAC,IAAI,EACpB,IAAI,CAACC,WAAW,GAAC,cAAa;IAClC,CAAC;IACDK,OAAOA,CAAA,EAAE;MACL,IAAI,CAACN,UAAU,GAAC,IAAI,EACpB,IAAI,CAACC,WAAW,GAAC,aAAY;IACjC,CAAC;IACDM,gBAAgBA,CAACC,OAAO,EAAE;MACtB;MACA,MAAMC,WAAU,GAAIb,QAAQ,CAACc,GAAG,CAACC,OAAO,CAACH,OAAO,EAAE,iBAAiB,CAAC,CAACI,QAAQ,CAAC,CAAC;MAC/E,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEC,IAAI,EAAE,cAAc;QAAEC,MAAM,EAAE;UAAER,OAAO,EAAEC;QAAY;MAAE,CAAC,CAAC;IACjF,CAAC;IACDN,WAAWA,CAAA,EAAE;MACTN,SAAS,CAACoB,GAAG,CAAC,kCAAkC,CAAC,EAAE,EA