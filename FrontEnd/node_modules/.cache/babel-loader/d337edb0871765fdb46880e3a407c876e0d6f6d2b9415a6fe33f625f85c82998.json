{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.find.js\";\nimport Axios from \"axios\";\nimport store from \"../../state/store\";\nimport apiClient from '@/plugins/axios';\nimport common from \"@/components/common/JavaScripCommon\";\nexport default {\n  data() {\n    return {\n      tinhData: [],\n      huyenData: [],\n      Gender: [{\n        title: 'Nam',\n        value: 1\n      }, {\n        title: 'Nữ',\n        value: 2\n      }],\n      selectTinh: null,\n      selectHuyen: null,\n      searchPrice: [0, 10000000],\n      status: false,\n      message: '',\n      snackbar: false,\n      snackbarColor: '',\n      customerId: 0,\n      indexSelect: 0,\n      searchGender: null,\n      PostData: []\n    };\n  },\n  name: \"LANDING\",\n  components: {},\n  created() {\n    this.customerId = store.getters['getCustomerId'] ?? 0;\n    this.GetTinh();\n  },\n  methods: {\n    GetTinh() {\n      Axios.get(`https://esgoo.net/api-tinhthanh/1/0.htm`).then(response => {\n        this.tinhData = response.data.data;\n      }).catch(error => {\n        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n      });\n    },\n    onTinhChange() {\n      Axios.get(`https://esgoo.net/api-tinhthanh/2/${this.selectTinh}.htm`).then(response => {\n        this.huyenData = response.data.data;\n      }).catch(error => {\n        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n      });\n    },\n    onHuyenChange() {\n      Axios.get(`https://esgoo.net/api-tinhthanh/3/${this.selectHuyen}.htm`).then(response => {\n        this.phuongData = response.data.data;\n      }).catch(error => {\n        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n      });\n    },\n    FormatPrice(price) {\n      return common.formatTablePrice(price);\n    },\n    FindPeople() {\n      const tinh = this.tinhData.find(item => item.id === this.selectTinh);\n      const huyen = this.huyenData.find(item => item.id === this.selectHuyen);\n      var searchaddress = '';\n      if (tinh && huyen) {\n        searchaddress = ` ${huyen.full_name} ${tinh.full_name}`;\n      } else {\n        this.message = \"Vui lòng chọn đầy đủ  địa chỉ trước \";\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n        return;\n      }\n      const priceArrive = this.searchPrice[1];\n      apiClient.get(`/Post/SearchPost`, {\n        params: {\n          address: searchaddress,\n          priceFrom: this.searchPrice[0],\n          priceArrive: priceArrive,\n          customerId: this.customerId ?? 0,\n          gender: this.searchGender ?? 0\n        }\n      }).then(response => {\n        this.PostData = response.data;\n        this.status = true;\n      }).catch(error => {\n        this.message = \"Lấy danh sách bị lỗi: \" + error.response?.data?.message || error;\n        this.snackbar = true;\n        this.snackbarColor = 'red';\n      });\n    },\n    GotoDetail(postId) {\n      const route = this.$router.resolve({\n        name: 'detailpost',\n        params: {\n          idpost: postId\n        }\n      });\n      window.open(route.href, '_blank');\n    },\n    SavePost(postId, status, index) {\n      // Hiển thị thông báo dựa trên trạng thái\n      if (status) {\n        this.message = \"Xoá phòng thành công\";\n      } else {\n        this.message = \"Lưu phòng thành công\";\n      }\n\n      // Đặt trạng thái đang xử lý\n      this.PostData[index].isProcessing = true;\n\n      // Gửi yêu cầu API\n      apiClient.put(`/Post/SavePost?customerId=${this.customerId}&postId=${postId}&status=${status}`).then(() => {\n        // Cập nhật trạng thái `isSave` sau khi API trả về thành công\n        this.PostData[index].isSave = !this.PostData[index].isSave;\n\n        // Hiển thị thông báo thành công\n        this.snackbar = true;\n        this.snackbarColor = \"green\";\n        this.indexSelect = index;\n      }).catch(error => {\n        // Xử lý lỗi\n        this.message = \"Lưu phòng đã xảy ra lỗi: \" + (error.response?.data?.message || error.message || \"Không xác định\");\n        this.snackbar = true;\n        this.snackbarColor = \"red\";\n      }).finally(() => {\n        // Đặt lại trạng thái xử lý\n        this.PostData[index].isProcessing = false;\n      });\n    }\n  }\n};","map":{"version":3,"names":["Axios","store","apiClient","common","data","tinhData","huyenData","Gender","title","value","selectTinh","selectHuyen","searchPrice","status","message","snackbar","snackbarColor","customerId","indexSelect","searchGender","PostData","name","components","created","getters","GetTinh","methods","get","then","response","catch","error","onTinhChange","onHuyenChange","phuongData","FormatPrice","price","formatTablePrice","FindPeople","tinh","find","item","id","huyen","searchaddress","full_name","priceArrive","params","address","priceFrom","gender","GotoDetail","postId","route","$router","resolve","idpost","window","open","href","SavePost","index","isProcessing","put","isSave","finally"],"sources":["D:\\Code\\BoardingHouseManagement\\BoardingHouseManagement\\FrontEnd\\src\\views\\FindRoom\\FindPeople.vue"],"sourcesContent":["<style scoped>\r\n    .saved-button {\r\n        color: red !important; /* Màu chữ trắng */\r\n    }\r\n</style>\r\n<script>\r\n    import Axios from \"axios\";\r\n    import store from \"../../state/store\";\r\n    import apiClient from '@/plugins/axios';\r\n    import common from \"@/components/common/JavaScripCommon\"\r\n\r\n    export default {\r\n        data() {\r\n            return{\r\n                tinhData: [],\r\n                huyenData: [],\r\n                Gender: [\r\n                    {title: 'Nam',value: 1},\r\n                    {title: 'Nữ',value: 2},\r\n                ],\r\n                selectTinh: null,\r\n                selectHuyen: null,\r\n                searchPrice: [0,10000000],\r\n                status: false,\r\n                message: '',\r\n                snackbar: false,\r\n                snackbarColor: '',\r\n                customerId: 0,\r\n                indexSelect: 0,\r\n                searchGender: null,\r\n                PostData:[],\r\n            }\r\n        },\r\n        name: \"LANDING\",\r\n        components: {\r\n            \r\n        },\r\n        created(){\r\n            this.customerId = store.getters['getCustomerId'] ?? 0;\r\n            this.GetTinh();\r\n        },\r\n        methods: {\r\n            GetTinh(){\r\n                Axios.get(`https://esgoo.net/api-tinhthanh/1/0.htm`)\r\n                    .then(response=>{\r\n                        this.tinhData =  response.data.data;\r\n                    })\r\n                    .catch(error=>{\r\n                        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\r\n                        this.snackbar = true;\r\n                        this.snackbarColor = 'red';\r\n                    })\r\n            },\r\n            onTinhChange(){\r\n                Axios.get(`https://esgoo.net/api-tinhthanh/2/${this.selectTinh}.htm`)\r\n                    .then(response=>{\r\n                        this.huyenData =  response.data.data;\r\n                    })\r\n                    .catch(error=>{\r\n                        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\r\n                        this.snackbar = true;\r\n                        this.snackbarColor = 'red';\r\n                    })\r\n            },\r\n            onHuyenChange(){\r\n                Axios.get(`https://esgoo.net/api-tinhthanh/3/${this.selectHuyen}.htm`)\r\n                    .then(response=>{\r\n                        this.phuongData =  response.data.data;\r\n                    })\r\n                    .catch(error=>{\r\n                        this.message = \"Lấy danh sách tỉnh thành bị lỗi \" + error;\r\n                        this.snackbar = true;\r\n                        this.snackbarColor = 'red';\r\n                    })\r\n            },\r\n            FormatPrice(price){\r\n                return common.formatTablePrice(price);\r\n            },\r\n            FindPeople(){\r\n                const tinh = this.tinhData.find(item => item.id === this.selectTinh);\r\n                const huyen = this.huyenData.find(item => item.id === this.selectHuyen);\r\n                var searchaddress ='';\r\n                \r\n                if (tinh && huyen) {\r\n                    searchaddress = ` ${huyen.full_name} ${tinh.full_name}`;\r\n                }\r\n                else{\r\n                    this.message = \"Vui lòng chọn đầy đủ  địa chỉ trước \";\r\n                    this.snackbar = true;\r\n                    this.snackbarColor = 'red';\r\n                    return;\r\n                }\r\n                const priceArrive = this.searchPrice[1]\r\n                apiClient.get(`/Post/SearchPost`,{\r\n                    params: {\r\n                        address: searchaddress,\r\n                        priceFrom: this.searchPrice[0],\r\n                        priceArrive: priceArrive,\r\n                        customerId: this.customerId ?? 0,\r\n                        gender : this.searchGender ?? 0,\r\n                    },\r\n                })\r\n                        .then(response=>{\r\n                            this.PostData = response.data\r\n                            this.status = true;\r\n                        })\r\n                        .catch(error=>{\r\n                            this.message = \"Lấy danh sách bị lỗi: \" + error.response?.data?.message || error;\r\n                            this.snackbar = true;\r\n                            this.snackbarColor = 'red';\r\n                        })\r\n            },\r\n            GotoDetail(postId){\r\n                const route = this.$router.resolve({ name: 'detailpost', params: { idpost: postId } });\r\n                window.open(route.href, '_blank');\r\n            },\r\n            SavePost(postId,status,index){\r\n                // Hiển thị thông báo dựa trên trạng thái\r\n                if (status) {\r\n                this.message = \"Xoá phòng thành công\";\r\n                } else {\r\n                this.message = \"Lưu phòng thành công\";\r\n                }\r\n\r\n                // Đặt trạng thái đang xử lý\r\n                this.PostData[index].isProcessing = true;\r\n\r\n                // Gửi yêu cầu API\r\n                apiClient\r\n                .put(`/Post/SavePost?customerId=${this.customerId}&postId=${postId}&status=${status}`)\r\n                .then(() => {\r\n                    // Cập nhật trạng thái `isSave` sau khi API trả về thành công\r\n                    this.PostData[index].isSave = !this.PostData[index].isSave;\r\n\r\n                    // Hiển thị thông báo thành công\r\n                    this.snackbar = true;\r\n                    this.snackbarColor = \"green\";\r\n                    this.indexSelect = index;\r\n                })\r\n                .catch((error) => {\r\n                    // Xử lý lỗi\r\n                    this.message =\r\n                    \"Lưu phòng đã xảy ra lỗi: \" +\r\n                    (error.response?.data?.message || error.message || \"Không xác định\");\r\n                    this.snackbar = true;\r\n                    this.snackbarColor = \"red\";\r\n                })\r\n                .finally(() => {\r\n                    // Đặt lại trạng thái xử lý\r\n                    this.PostData[index].isProcessing = false;\r\n                });\r\n            },\r\n        },\r\n    }\r\n</script>\r\n<template>\r\n    <v-container class=\"py-0\">\r\n        <v-snackbar\r\n            v-model=\"snackbar\"\r\n            :timeout=\"10000\"\r\n            class=\"custom-snackbar\"\r\n            :color=\"snackbarColor\"\r\n        >\r\n            <h5 class=\"text-center\">{{ message }}</h5>\r\n        </v-snackbar>\r\n            <BCard>\r\n                <BRow>\r\n                    <BCol class=\"col-xl-3 col-md-4 col-12\">\r\n                        <div class=\"form-group mt-lg-3\">\r\n                            <label class=\"form-label\">Chọn tỉnh, thành phố</label>\r\n                            <v-autocomplete\r\n                                v-model=\"selectTinh\"\r\n                                :items=\"tinhData\"\r\n                                item-title=\"full_name\"\r\n                                item-value=\"id\"\r\n                                outlined\r\n                                dense\r\n                                clearable\r\n                                @update:modelValue=\"onTinhChange\"\r\n                            ></v-autocomplete>\r\n                        </div>\r\n                    </BCol>\r\n                    <BCol class=\"col-xl-3 col-md-4 col-12\">\r\n                        <div class=\"form-group mt-lg-3\">\r\n                            <label class=\"form-label\">Quận, huyện</label>\r\n                            <v-autocomplete\r\n                                v-model=\"selectHuyen\"\r\n                                :items=\"huyenData\"\r\n                                item-title=\"full_name\"\r\n                                item-value=\"id\"\r\n                                outlined\r\n                                dense\r\n                                clearable\r\n                                :disabled=\"!selectTinh\"\r\n                                @update:modelValue=\"onHuyenChange\"\r\n                            ></v-autocomplete>\r\n                        </div>\r\n                    </BCol>\r\n                    <BCol class=\"col-xl-4 col-md-8\">\r\n                        <div class=\"form-group m-0\">\r\n                            <label class=\"form-label\">Giá phòng</label>\r\n                            <v-range-slider\r\n                                v-model=\"searchPrice\"\r\n                                range\r\n                                min=\"0\"\r\n                                max=\"10000000\"\r\n                                step=\"1000\"\r\n                                color=\"primary\"\r\n                                hide-details\r\n                            ></v-range-slider>\r\n                            <BRow>\r\n                                <BCol class=\"col-6\">\r\n                                    <v-text-field\r\n                                        label=\"Từ\"\r\n                                        v-model=\"searchPrice[0]\"\r\n                                        :type=\"'number'\"\r\n                                        hide-details\r\n                                        suffix=\"VNĐ\"\r\n                                    ></v-text-field>\r\n                                </BCol>\r\n                                <BCol class=\"col-6\">\r\n                                    <v-text-field\r\n                                        label=\"Đến\"\r\n                                        v-model=\"searchPrice[1]\"\r\n                                        :type=\"'text'\"\r\n                                        hide-details\r\n                                        suffix=\"VNĐ\"\r\n                                    ></v-text-field>\r\n                                </BCol>\r\n                            </BRow>\r\n                        </div>\r\n                    </BCol>\r\n                    <BCol class=\"col-xl-2 col-md-8\">\r\n                        <div class=\"form-group mt-lg-3\">\r\n                            <label class=\"form-label\">Giới tính</label>\r\n                            <v-select\r\n                                clearable\r\n                                :items=\"Gender\"\r\n                                item-title=\"title\"\r\n                                item-value=\"value\"\r\n                                v-model=\"searchGender\"\r\n                                variant=\"outlined\"\r\n                                hide-details>\r\n                            </v-select>\r\n                        </div>\r\n                    </BCol>\r\n                    <BCol class=\"col-xl-12 col-md-4 col-12 d-flex justify-center align-center\">\r\n                        <v-btn class=\"mt-3 mt-md-0\" prepend-icon=\"mdi-magnify\" @click=\"FindPeople()\">Tìm người ở ghép</v-btn>\r\n                    </BCol>\r\n                </BRow>\r\n            </BCard>\r\n        </v-container>\r\n\r\n        <BContainerFluid class=\"container-fluid\">\r\n            <div class=\"bg-dark mx-sm-3 home-section justify-content-center\" v-if=\"!status\">\r\n               <div class=\"swiper language-slides-hero m-0\">\r\n                   <div class=\"swiper-wrapper \" >\r\n                       <swiper class=\"default-swiper rounded\"  :autoplay=\"{\r\n                           delay: 2500,\r\n                           disableOnInteraction: false,\r\n                       }\">\r\n                           <swiper-slide>\r\n                               <BRow class=\"align-items-center justify-content-center text-center\">\r\n                                   <BCol sm=\"12\" class=\"header-content\">\r\n                                       <BRow class=\"justify-content-center text-center\">\r\n                                           <BCol lg=\"8\" sm=\"10\" Cols=\"11\" class=\"col-xl-7 col-md-9\">\r\n                                               <h1 class=\"my-3 wow animate__fadeInUp text-white\" data-wow-delay=\"0.4s\">Chào mừng bạn đến với trang web tìm kiếm người ở ghép.</h1>\r\n                                           </BCol>\r\n                                       </BRow>\r\n                                       <BRow class=\"justify-content-center text-center\">\r\n                                           <BCol lg=\"7\" sm=\"10\" Cols=\"11\" class=\"col-xxl-5 col-xl-6 col-md-8\">\r\n                                               <p class=\"f-16 mb-3 mb-lg-5 wow animate__fadeInUp\" data-wow-delay=\"0.6s\">Bạn vui lòng chọn địa chỉ trên thanh tìm kiếm để tìm người ở ghép phù hợp cho bạn.</p>\r\n                                           </BCol>\r\n                                       </BRow>\r\n                                   </BCol>\r\n                               </BRow>\r\n                           </swiper-slide>\r\n                       </swiper>\r\n                   </div>\r\n               </div>               \r\n           </div>\r\n            <div class=\"mx-sm-3\" v-else>\r\n                <v-container class=\"pt-0\">\r\n                    <v-row>\r\n                        <v-col md=\"2\"></v-col>\r\n                        <h1 v-if=\"postData == 0\">Bạn không có bài đăng nào</h1>\r\n                        <v-col cols=\"12\" md=\"8\" v-else>\r\n                            <h3>Danh sách các bài đăng tìm người ở ghép</h3>\r\n                            <BCard no-body v-for=\"(post, index) in PostData\" :key=\"index\">\r\n                                <BCardBody class=\"p-0\">\r\n                                    <BRow>\r\n                                        <BCol class=\"col-md-4 col-12\">\r\n                                            <v-window style=\"height: 100%;\"\r\n                                                v-model=\"post.selectimg\"\r\n                                                show-arrows\r\n                                                theme=\"dark\"\r\n                                            >\r\n                                                <v-window-item\r\n                                                v-for=\"(item,index) in post.img\"\r\n                                                :key=\"inde