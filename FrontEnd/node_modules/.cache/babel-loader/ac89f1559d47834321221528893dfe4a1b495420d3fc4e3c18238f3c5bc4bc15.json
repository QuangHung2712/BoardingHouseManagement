wrapper \" >\r\n                        <swiper class=\"default-swiper rounded\"  :autoplay=\"{\r\n                            delay: 2500,\r\n                            disableOnInteraction: false,\r\n                        }\">\r\n                            <swiper-slide>\r\n                                <BRow class=\"align-items-center justify-content-center text-center\">\r\n                                    <BCol sm=\"12\" class=\"header-content\">\r\n                                        <BRow class=\"justify-content-center text-center\">\r\n                                            <BCol lg=\"8\" sm=\"10\" Cols=\"11\" class=\"col-xl-7 col-md-9\">\r\n                                                <h1 class=\"my-3 wow animate__fadeInUp text-white\" data-wow-delay=\"0.4s\">Chào mừng bạn đến với trang web tìm kiếm nhà trọ.</h1>\r\n                                            </BCol>\r\n                                        </BRow>\r\n                                        <BRow class=\"justify-content-center text-center\">\r\n                                            <BCol lg=\"7\" sm=\"10\" Cols=\"11\" class=\"col-xxl-5 col-xl-6 col-md-8\">\r\n                                                <p class=\"f-16 mb-3 mb-lg-5 wow animate__fadeInUp\" data-wow-delay=\"0.6s\">Bạn vui lòng chọn địa chỉ trên thanh tìm kiếm để tìm phòng trọ phù hợp cho bạn.</p>\r\n                                            </BCol>\r\n                                        </BRow>\r\n                                    </BCol>\r\n                                </BRow>\r\n                            </swiper-slide>\r\n                        </swiper>\r\n                    </div>\r\n                </div>               \r\n            </div>\r\n            <div class=\"mx-sm-3 ListRoom\" v-else>\r\n                <v-container class=\"pt-0\">\r\n                    <v-row>\r\n                        <v-col md=\"1\"></v-col>\r\n                        <v-col cols=\"12\" md=\"10\">\r\n                            <h3>Danh sách các phòng trọ</h3>\r\n                            <BCard no-body v-for=\"(room, index) in roomData\" :key=\"index\">\r\n                                <BCardBody class=\"p-0\">\r\n                                    <BRow>\r\n                                        <BCol class=\"col-md-4 col-12\">\r\n                                            <v-window style=\"height: 100%;\"\r\n                                                v-model=\"window\"\r\n                                                show-arrows\r\n                                                theme=\"dark\"\r\n                                            >\r\n                                                <v-window-item\r\n                                                v-for=\"(item,index) in room.img\"\r\n                                                :key=\"index\"\r\n                                                >\r\n                                                <v-card class=\"d-flex justify-center align-center\" height=\"200px\">\r\n                                                    <v-img\r\n                                                        :src=\"item.img\"\r\n                                                        alt=\"Room Image\"\r\n                                                        aspect-ratio=\"16/9\"\r\n                                                        class=\"rounded elevation-2\"\r\n                                                        contain\r\n                                                    ></v-img>\r\n                                                </v-card>\r\n                                                </v-window-item>\r\n                                            </v-window>\r\n                                        </BCol>\r\n                                        <BCol class=\"col-md-8 col-12 mt-4\">\r\n                                            <router-link to=\"/detail\" target=\"_blank\">\r\n                                                <h4> {{ room.towerName }}</h4>\r\n                                                <h5>Giá: <span style=\"color: red;\">{{ FormatPrice(room.price) }}</span></h5>\r\n                                                <p><v-icon>mdi-map-marker-radius</v-icon> {{ room.towerAddress }} </p>\r\n                                                <p>Thiết bị: {{ room.device }}</p>\r\n                                            </router-link>\r\n                                            <div class=\"text-right\">\r\n                                                <BButton variant=\"white\"><v-icon >mdi-heart-outline</v-icon></BButton>\r\n                                            </div>\r\n                                        </BCol>\r\n                                    </BRow>\r\n                                </BCardBody>\r\n                            </BCard>\r\n                        </v-col>\r\n                        <v-col xl=\"1\"></v-col>\r\n                    </v-row>\r\n                </v-container>\r\n            </div>\r\n\r\n        </BContainerFluid>\r\n</template>"],"mappings":";;AACA,SAASA,MAAM,EAAEC,WAAU,QAAS,YAAY;AAEhD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,MAAK,MAAO,qCAAoC;AAGvD,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAM;MACFC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,IAAI;MAChBC,WAAW,EAAE,IAAI;MACjBC,WAAW,EAAE,CAAC,CAAC,EAAC,QAAQ,CAAC;MACzBC,MAAM,EAAE,IAAI;MACZC,MAAM,EAAE,CAAC;MACTC,MAAM,EAAE,CAAC;MACTC,QAAO,EAAG,EAET;MACDC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,KAAK;MACfC,aAAa,EAAE;IACnB;EACJ,CAAC;EACDC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACRnB,MAAM;IACNC;EAEJ,CAAC;EACDmB,OAAOA,CAAA,EAAE;IACL,IAAI,CAACC,OAAO,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACLD,OAAOA,CAAA,EAAE;MACLnB,KAAK,CAACqB,GAAG,CAAC,yCAAyC,EAC9CC,IAAI,CAACC,QAAQ,IAAE;QACZ,IAAI,CAACnB,QAAO,GAAKmB,QAAQ,CAACpB,IAAI,CAACA,IAAI;MACvC,CAAC,EACAqB,KAAK,CAACC,KAAK,IAAE;QACV,IAAI,CAACZ,OAAM,GAAI,kCAAiC,GAAIY,KAAK;QACzD,IAA