{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Rightbar from \"@/components/right-bar.vue\";\nimport apiClient from \"@/plugins/axios\";\nexport default {\n  name: \"LOGIN\",\n  components: {\n    Rightbar\n  },\n  data() {\n    return {\n      login: {\n        email: '',\n        password: ''\n      },\n      remember: false,\n      form: false,\n      showPassword: false,\n      errorMessage: ''\n    };\n  },\n  created() {\n    const token = localStorage.getItem('tokenlandlord');\n    if (token) {\n      // Token tồn tại, chuyển hướng đến trang tower\n      this.$router.push({\n        name: 'tower'\n      });\n    }\n  },\n  methods: {\n    required(v) {\n      return !!v || 'Vui lòng không để trống';\n    },\n    LogIn() {\n      apiClient.post(`/Landlord/Login`, this.login).then(response => {\n        this.$store.dispatch('login', response.data);\n        this.$router.push({\n          name: 'tower'\n        });\n        if (this.remember) {\n          localStorage.setItem('tokenlandlord', response.data.token);\n        }\n        this.errorMessage = \"\";\n      }).catch(error => {\n        if (error.response && error.response.status === 401) {\n          this.errorMessage = error.response.data.message;\n        } else {\n          this.errorMessage = 'Đã xảy ra lỗi hệ thống. ' + error.response.data.message;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["Rightbar","apiClient","name","components","data","login","email","password","remember","form","showPassword","errorMessage","created","token","localStorage","getItem","$router","push","methods","required","v","LogIn","post","then","response","$store","dispatch","setItem","catch","error","status","message"],"sources":["D:\\Code\\BoardingHouseManagement\\BoardingHouseManagement\\FrontEnd\\src\\views\\Manage\\Auth\\Login.vue"],"sourcesContent":["<script>\r\nimport Rightbar from \"@/components/right-bar.vue\"\r\nimport apiClient from \"@/plugins/axios\";\r\n\r\nexport default {\r\n    name: \"LOGIN\",\r\n    components: {\r\n        Rightbar\r\n    },\r\n    data(){\r\n        return{\r\n            login:{\r\n                email:'',\r\n                password:'',\r\n            },\r\n            remember: false,\r\n            form: false,\r\n            showPassword: false,\r\n            errorMessage: '',\r\n        }\r\n    },\r\n    created() {\r\n        const token = localStorage.getItem('tokenlandlord');\r\n        if (token) {\r\n            // Token tồn tại, chuyển hướng đến trang tower\r\n            this.$router.push({ name: 'tower' });\r\n        }\r\n    },\r\n    methods:{\r\n        required (v) {\r\n            return !!v || 'Vui lòng không để trống'\r\n        },\r\n        LogIn() {\r\n            apiClient.post(`/Landlord/Login`,this.login)\r\n                .then(response=>{\r\n                    this.$store.dispatch('login',response.data);\r\n                    this.$router.push({ name: 'tower' });\r\n                    if(this.remember){\r\n                        localStorage.setItem('tokenlandlord',response.data.token)\r\n                    }\r\n                    this.errorMessage = \"\"\r\n                })\r\n                .catch(error=>{\r\n                    if (error.response && error.response.status === 401) {\r\n                        this.errorMessage = error.response.data.message ;\r\n                    } else {\r\n                        this.errorMessage = 'Đã xảy ra lỗi hệ thống. '+ error.response.data.message;\r\n                    }\r\n                })\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<template>\r\n    <div class=\"auth-main v2\">\r\n        <div class=\"bg-overlay bg-dark\"></div>\r\n        <div class=\"auth-wrapper\">\r\n            <div class=\"auth-sidecontent\">\r\n\r\n                <div class=\"auth-sidefooter\">\r\n                    <h4 class=\"text-white\">QUẢN LÝ NHÀ TRỌ</h4>\r\n                    <!-- <img src=\"\" class=\"img-brand img-fluid\" alt=\"images\"> -->\r\n                    <hr class=\"mb-3 mt-4\">\r\n                    <!-- <BRow class=\"row\">\r\n                        <BCol class=\"col-auto my-1\">\r\n                            <ul class=\"list-inline footer-link mb-0\">\r\n                                <li class=\"list-inline-item\"><router-link to=\"/dashboard\">Home</router-link></li>\r\n                                <li class=\"list-inline-item\"><a href=\"#\"\r\n                                        target=\"_blank\">Documentation</a></li>\r\n                                <li class=\"list-inline-item\"><a href=\"#\"\r\n                                        target=\"_blank\">Support</a></li>\r\n                            </ul>\r\n                        </BCol>\r\n                    </BRow> -->\r\n                </div>\r\n            </div>\r\n            <div class=\"auth-form\">\r\n                <div class=\"card my-5 mx-3\">\r\n                    <div class=\"card-body\">\r\n                        <h4 class=\"f-w-500 mb-1\">Đăng nhập</h4>\r\n                        <p class=\"mb-3\">Bạn chưa có tài khoản? <router-link to=\"/register\"\r\n                                class=\"link-primary ms-1\">Tạo tài khoản</router-link></p>\r\n                        <v-form v-model=\"form\" ref=\"form\" @keyup.enter=\"LogIn()\">\r\n                            <div class=\"form-group mb-3\">\r\n                                <v-text-field \r\n                                    type=\"email\" \r\n                                    label=\"Email\"  \r\n                                    variant=\"outlined\" \r\n                                    placeholder=\"Email\" \r\n                                    :rules=\"[required]\" \r\n                                    v-model=\"login.email\"\r\n                                    prepend-inner-icon=\"mdi-email-outline\"\r\n                                ></v-text-field>\r\n                            </div>\r\n                                <v-text-field \r\n                                    :type=\"showPassword ? 'text' : 'password'\" \r\n                                    label=\"Mật khẩu\"  \r\n                                    variant=\"outlined\"\r\n                                    placeholder=\"Mật khẩu\" \r\n                                    :rules=\"[required]\" \r\n                                    v-model=\"login.password\"\r\n                                    prepend-inner-icon=\"mdi-lock-outline\"\r\n                                    :append-inner-icon=\"showPassword ? 'mdi-eye' : 'mdi-eye-off'\"\r\n                                    @click:append-inner=\"showPassword = !showPassword\"\r\n                                    ></v-text-field>\r\n                            <div class=\"d-flex mt-1 justify-content-between align-items-center\">\r\n                                <div class=\"form-check\">\r\n                                    <input class=\"form-check-input input-primary\" type=\"checkbox\" id=\"customCheckc1\" v-model=\"remember\">\r\n                                    <label class=\"form-check-label text-muted\" for=\"customCheckc1\">Nhớ mật khẩu</label>\r\n                                </div>\r\n                                <router-link to=\"/forgot\">\r\n                                    <h6 class=\"text-secondary f-w-400 mb-0\">Quên mật khẩu?</h6>\r\n                                </router-link>\r\n                            </div>\r\n                        </v-form>\r\n                        <div class=\"d-grid mt-4\">\r\n                            <button type=\"button\" @click=\"LogIn()\" class=\"btn btn-primary\" :disabled=\"!form\">Đăng nhập</button>\r\n                            <p v-if=\"errorMessage\" class=\"text-danger mt-2\">{{ errorMessage }}</p>\r\n                        </div>\r\n                        <div class=\"saprator my-3\">\r\n                            <span>Đăng nhập bằng</span>\r\n        